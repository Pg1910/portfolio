<svg width="32" height="32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="roboticGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00d4ff;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#6366f1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  <rect width="32" height="32" fill="url(#roboticGrad)"/>
  <!-- Circuit board pattern -->
  <rect x="4" y="4" width="24" height="24" fill="none" stroke="white" stroke-width="1" opacity="0.3"/>
  <rect x="8" y="8" width="16" height="16" fill="none" stroke="white" stroke-width="1" opacity="0.2"/>
  <!-- Robot eye -->
  <circle cx="16" cy="16" r="6" fill="white" opacity="0.9"/>
  <circle cx="16" cy="16" r="3" fill="#00d4ff"/>
  <circle cx="16" cy="16" r="1" fill="white"/>
  <!-- Circuit lines -->
  <line x1="4" y1="12" x2="8" y2="12" stroke="white" stroke-width="1" opacity="0.6"/>
  <line x1="24" y1="20" x2="28" y2="20" stroke="white" stroke-width="1" opacity="0.6"/>
  <line x1="12" y1="4" x2="12" y2="8" stroke="white" stroke-width="1" opacity="0.6"/>
  <line x1="20" y1="24" x2="20" y2="28" stroke="white" stroke-width="1" opacity="0.6"/>
</svg>
